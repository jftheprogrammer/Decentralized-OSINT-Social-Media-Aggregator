{
  "sources": [
    {
      "name": "Twitter",
      "endpoint": "https://api.twitter.com/2/tweets/search/recent",
      "params": {
        "query": "OSINT",
        "max_results": 10
      },
      "headers": {
        "Authorization": "Bearer YOUR_TWITTER_API_TOKEN"
      }
    },
    {
      "name": "Reddit",
      "endpoint": "https://www.reddit.com/search.json",
      "params": {
        "q": "OSINT",
        "limit": 10
      }
    },
    {
      "name": "YouTube",
      "endpoint": "https://www.googleapis.com/youtube/v3/search",
      "params": {
        "q": "OSINT",
        "part": "snippet",
        "maxResults": 10,
        "key": "YOUR_YOUTUBE_API_TOKEN"
      }
    }
  ],
  "rate_limit": 5,
  "cache_ttl": 300
}
